.version 0
.chunk "truffleshuffle"
.constants
0 "1..1\n"
1 "n"
2 "ok 1 add_n: 42.0 +  5.0 = 47.0\n"
3 42.0
4 5.0
5 47.0
.metadata
.bytecode

    set_imm S0, 0, 0
    deref   S0, CONSTS, S0
    set_imm I0, 0, 1
    print_s I0, S0, x

    set_imm N0, 0, 3
    deref   N0, CONSTS, N0
    set_imm I1, 0, 0
    deref   N0, N0, I1
    set_imm N1, 0, 4
    deref   N1, CONSTS, N1
    deref   N1, N1, I1
    add_n   N2, N1, N0
    set_imm N3, 0,  5
    deref   N3, CONSTS, N3
    deref   N3, N3, I1
    sub_n   N4, N3, N2

    goto_if addn_nok, N4
    goto    addn_ok, x

addn_nok:
    set_imm S0, 0, 1
    deref   S0, CONSTS, S0
    print_s I0, S0, x

addn_ok:
    set_imm S0, 0, 2
    deref   S0, CONSTS, S0
    print_s I0, S0, x

